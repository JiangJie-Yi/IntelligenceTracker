    "nodemon": "^3.1.10",
    "npm": "^11.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.5.2",
    "styled-components": "^6.1.17",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0",
    "tsx": "^4.19.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.15.3",
